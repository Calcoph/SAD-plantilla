{
    "$schema": "schema_config.json",
    "input_file": "TweetsTrainDev.csv",
    "columns": {
        "list_type": "including",
        "columns": ["airline_sentiment", "text"]
    },
    "categorical_columns": [],
    "text_columns": [],
    "numerical_columns": [],
    "predict_column": "airline_sentiment",
    "nlp_columns": ["text"],
    "target_map": {
        "neutral": 0,
        "positive": 1,
        "negative": 2
    },
    "drop_rows_when_missing": [],
    "impute_when_missing": [
    ],
    "test_size": 0.2,
    "rescale_features": [
    ],
    "algorithms": [
        {
            "algorithm": "NaiveBayes",
            "maximize_output": {
                "property": "f1-score",
                "average": "macro"
            },
            "name": "NB_gaussian_model.sav"
        }
    ],
    "naive_bayes_config": {
        "type": "gaussian"
    }
}
